          "dialect": "oracle"
        }
      }
    },
    "task_solutions": {
      "query": [
        {
          "type": "query",
          "material_information": {
            "query": "SELECT Nachname FROM Kunde WHERE UPPER(Ort) = 'MAIESKUEL' AND FLOOR((SYSDATE - Geburtsdatum) / 365) > 35;",
            "dialect": "oracle"
          }
        }
      ]
    }
