      "problem_statement": [
        {
          "type": "text",
          "material_information": {
            "text": "Zeigen Sie die Nachnamen aller Kunden aus Maieskuel an, die älter als 35 Jahre sind."
          }
        }
      ],
      "db_schema": {
        "type": "schema",
        "material_information": {
